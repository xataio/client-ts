    "openapi-gen-control": "openapi-codegen controlPlane",
    "openapi-gen-data": "openapi-codegen dataPlane",
    "openapi-gen": "pnpm run openapi-gen-control && pnpm run openapi-gen-data"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xataio/client-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/xataio/client-ts/issues"
  },
  "homepage": "https://github.com/xataio/client-ts#readme",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@openapi-codegen/cli": "^2.0.0",
    "@openapi-codegen/typescript": "^6.2.1",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.38.0",
    "@opentelemetry/instrumentation": "^0.38.0",
    "@opentelemetry/resources": "^1.12.0",
    "@opentelemetry/sdk-trace-base": "^1.12.0",
    "@opentelemetry/sdk-trace-node": "^1.12.0",
    "@opentelemetry/semantic-conventions": "^1.12.0",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cross-fetch": "^3.1.5",
    "doctoc": "^2.2.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "msw": "^1.2.1",
    "node-fetch": "=2.6.7",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "rollup": "^3.20.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-strip-code": "^0.2.7",
    "size-limit": "^8.2.4",
    "ts-node": "^10.9.1",
    "turbo": "^1.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1",
    "zod": "^3.21.4"
  },
  "lint-staged": {
    "codegen/src/*.{js,ts,tsx}": [
      "npm run build-example",
      "git add codegen/example"
    ],
    "*.{js,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "client/{README,Usage}.md": [
      "doctoc --notitle"
    ]
  },
  "size-limit": [
    {
      "path": "packages/client/dist/index.mjs"
    },
    {
      "path": "packages/client/dist/index.cjs"
    },
    {
      "path": "packages/codegen/dist/index.mjs"
    },
    {
      "path": "packages/codegen/dist/index.cjs"
    }
  ]
}
